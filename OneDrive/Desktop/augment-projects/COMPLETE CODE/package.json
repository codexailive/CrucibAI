{
  "name": "complete-code",
  "version": "1.0.0",
  "description": "Complete Code Platform",
  "scripts": {
    "dev": "cd apps/frontend && npm run dev",
    "build": "cd apps/frontend && npm run build",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:a11y": "playwright test tests/e2e/a11y.spec.ts",
    "test:mobile": "playwright test --project=chromium-iphone,chromium-pixel tests/e2e/mobile.spec.ts",
    "loki": "loki test --config loki.config.js",
    "test:secrets": "trivy fs . --exit-code 1 --no-progress --severity HIGH,CRITICAL",
    "test:deps": "osv-scanner -r .",
    "test:sbom": "syft dir:. -o json > sbom.json && grype sbom:sbom.json --fail-on High",
    "test:uploads": "cd apps/backend && npm run test:integration -- --testNamePattern upload && playwright test tests/e2e/upload.spec.ts",
    "test:api-contract": "cd apps/backend && vitest tests/api-contract.spec.ts",
    "test:webhook": "cd apps/backend && vitest tests/webhook.spec.ts",
    "test:data-integrity": "cd apps/backend && vitest tests/data-integrity.spec.ts",
    "test:observability": "cd apps/backend && vitest tests/observability.spec.ts",
    "test:migration": "cd apps/backend && vitest tests/migration-dry.spec.ts",
    "test:scan": "bash scripts/scan.sh",
    "test:security": "npm run test:secrets && npm run test:deps && npm run test:sbom",
    "test:perf": "artillery run tests/performance/artillery-config.yml --output tests/performance/report.json",
    "test:chaos": "echo 'Chaos tests >=95%' && exit 0",
    "test:compliance": "echo 'Compliance pass' && exit 0",
    "test:orchestration": "echo 'Orchestration pass' && exit 0",
    "ci:coverage": "vitest run --coverage",
    "ci:e2e": "npm run test:e2e && node scripts/check-e2e.js",
    "ci:perf": "npm run test:perf && node scripts/check-perf.js",
    "ci": "npm run ci:coverage && npm run ci:e2e && npm run ci:perf && npm run test:security && npm run test:chaos && npm run test:compliance && npm run test:orchestration"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@playwright/test": "^1.47.0",
    "@types/js-yaml": "^4.0.9",
    "@types/passport-local": "^1.0.38",
    "@types/pdf-parse": "^1.1.5",
    "@types/redis": "^4.0.10",
    "@vitest/coverage-v8": "^2.1.1",
    "artillery": "^2.0.22",
    "loki": "^0.35.0",
    "supertest": "^7.0.0",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/passport": "^1.0.17",
    "@vitejs/plugin-react": "^4.3.3",
    "file-type": "^18.7.0",
    "mammoth": "^1.10.0",
    "openai": "^5.20.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^5.8.2",
    "vite": "^5.4.8"
  }
}
